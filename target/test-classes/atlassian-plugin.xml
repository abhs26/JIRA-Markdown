<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="com.learnLab.markdown.markdownPlugin-tests" name="markdownPlugin" plugins-version="2">
  <plugin-info>
    <description>This is the com.learnLab.markdown:markdownPlugin plugin for Atlassian JIRA.</description>
    <version>1.0</version>
    <vendor name="LearningLab" url="http://www.abhisheksrivastava.co/"/>
  </plugin-info>
  <!-- from our base plugin -->
  <component-import key="myComponent" interface="com.learnLab.markdown.api.MyPluginComponent"/>
  <!-- from the product container -->
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>
  <component key="my-component-wired-test" class="it.com.learnLab.markdown.MyComponentWiredTest" public="true">
    <interface>it.com.learnLab.markdown.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
